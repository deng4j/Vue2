<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot_shop.mapper.SpCategoryMapper">

    <resultMap type="com.example.springboot_shop.domain.SpCategory" id="SpCategoryResult">
            <result property="catId" column="cat_id"/>
            <result property="catDeleted" column="cat_deleted"/>
            <result property="catIcon" column="cat_icon"/>
            <result property="catLevel" column="cat_level"/>
            <result property="catName" column="cat_name"/>
            <result property="catPid" column="cat_pid"/>
            <result property="catSrc" column="cat_src"/>
    </resultMap>

</mapper>