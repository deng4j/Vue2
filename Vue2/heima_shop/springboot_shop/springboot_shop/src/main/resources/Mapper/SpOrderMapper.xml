<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.springboot_shop.mapper.SpOrderMapper">

    <resultMap type="com.example.springboot_shop.domain.SpOrder" id="SpOrderResult">
            <result property="createTime" column="create_time"/>
            <result property="orderNumber" column="order_number"/>
            <result property="orderId" column="order_id"/>
            <result property="consigneeAddr" column="consignee_addr"/>
            <result property="isSend" column="is_send"/>
            <result property="orderFapiaoCompany" column="order_fapiao_company"/>
            <result property="orderFapiaoContent" column="order_fapiao_content"/>
            <result property="orderFapiaoTitle" column="order_fapiao_title"/>
            <result property="orderPay" column="order_pay"/>
            <result property="orderPrice" column="order_price"/>
            <result property="payStatus" column="pay_status"/>
            <result property="tradeNo" column="trade_no"/>
            <result property="updateTime" column="update_time"/>
            <result property="userId" column="user_id"/>
    </resultMap>

</mapper>